{
  "name": "@feedefy/react",
  "type": "module",
  "version": "1.0.0",
  "description": "Embed the Feedefy widget in your React App",
  "author": "feedefy",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "microbundle",
    "dev": "microbundle watch"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "react": "^18.0.0",
    "simple-git-hooks": "^2.9.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
